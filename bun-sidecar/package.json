{
    "name": "bun-react-template",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "bun run build:css && bun --hot src/server.ts",
        "build": "bunx tailwindcss -i ./src/input.css -o ./src/output.css && bun eslint ./src && bun run tsc --skipLibCheck --noEmit",
        "build:css": "bunx tailwindcss -i ./src/input.css -o ./src/output.css",
        "build:css:dev": "bunx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
        "build:css:full": "bunx tailwindcss -i ./src/input.css -o ./src/output.css --content './src/**/*.{js,ts,jsx,tsx}' --safelist '*'",
        "logs": "bun tail-logs.ts",
        "agent": "bun run scripts/run-agent.ts",
        "build-dictionary": "bun run scripts/build-dictionary.ts"
    },
